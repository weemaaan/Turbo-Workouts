        </IntervalsT>
        <SteadyState show_avg="1" Power="1.149" Duration="180"/>
        <SteadyState show_avg="1" Power="0.4" Duration="120"/>
        <SteadyState show_avg="1" Power="0.7" Duration="720"/>
        <SteadyState show_avg="1" Power="0.4" Duration="120"/>
