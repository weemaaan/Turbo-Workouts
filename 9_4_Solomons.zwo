        <SteadyState show_avg="1" Power="0.4" Duration="60"/>
        <SteadyState show_avg="1" Power="1.2" Duration="60"/>
        <SteadyState show_avg="1" Power="0.4" Duration="60"/>
        <SteadyState show_avg="1" Power="1.35" Duration="30"/>
        <SteadyState show_avg="1" Power="0.4" Duration="270"/>
        <IntervalsT OnPower="1.149" OffPower="0.4" show_avg="1" Repeat="5" OffDuration="180" OnDuration="180">
